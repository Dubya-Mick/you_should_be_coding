{
  "manifest_version": 3,
  "name": "Bernitt!",
  "version": "1.0",
  "description": "A great tool to help you focus",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["./main.js"]
    }
  ],
  "web_accessible_resources": [{
    "matches": ["<all_urls>"],
    "resources": ["cat.jpg", "cat2.jpg"]
  }]
}